@page
@model Mokyklu_IS.Pages.LoginModel
@{
    ViewData["Title"] = "Log-In";
}

<div class="text-center">
    <form action="">
        <div class="container">
            <h1>Prisijungimas</h1>
            <hr>

            <label for="prisijungimo_vardas"><b>Prisijungimo Vardas</b></label><br>
            <input type="text" placeholder="Iveskite Prisijungimo Varda" name="pris_vardas" id="pris_vardas" required><br><br>

            <label for="psw"><b>Slaptazodis</b></label><br>
            <input type="password" placeholder="Iveskite Slaptazodi" name="psw" id="psw" required><br><br>

            <hr>

            <button type="submit" class="registerbtn">Prisijungti</button>
        </div>
    </form>
</div>
