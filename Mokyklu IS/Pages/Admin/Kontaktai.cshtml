@page
@model Mokyklu_IS.Pages.Admin.KontaktaiModel
@{
    Layout = "/Pages/Shared/_AdminLayout.cshtml";
}
<div class="text-center">
    <div class="container">
        <h3 class="display-4">Kontaktai</h3>
        @if (Model.Registracija.Count() > 0)
        {
        <table id="customers">
            <tr>
                <th>Vardas</th>
                <th>Pavardė</th>
                <th>Tipas</th>
                <th>Telefono-Nr</th>
            </tr>
            @foreach (var item in Model.Registracija)
            {
            <tr>
                <td>@Html.DisplayFor(m => item.Vardas)</td>
                <td>@Html.DisplayFor(m => item.Pavarde)</td>
                <td>@Html.DisplayFor(m => item.Role)</td>
                <td>@Html.DisplayFor(m => item.Telefono_nr)</td>
            </tr>
            }
        </table>
        }
            else
            {
                <p>Nerasta jokiu kontaktu</p>
            }
    </div>
</div>

